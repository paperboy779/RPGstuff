            <string>/// Starting someshit or something
scr_menuContainers();
//Drawing main
draw_rectangle_colour(
    contMainX,
    contMainY,
    contMainX2,
    contMainY2,
    colPrimary,
    colPrimary,
    colSecondary,
    colSecondary,
    false
    );


// Drawing primary pause menu container
draw_rectangle_colour(
    contPauseX,
    contPauseY,
    contPauseX2,
    contPauseY2,
    colPrimary,
    colPrimary,
    colPrimary,
    colPrimary,
    false
    );
// For loop to make buttons
draw_set_font(font_Main);
for (i=0; i&lt;array_height_2d(pausedMenu); i+=1)
    {
    buttonY = ((i * (string_height(pausedMenu[i,0]) + ((contPauseY2 - contPauseY) * .01))) + 1 );
    buttonH = ((i + 1) * (string_height(pausedMenu[i,0]) + ((contPauseY2 - contPauseY) * .01)));
    draw_set_color(colPrimary);
    if (i == selectionArray[selectionActive,0])
        {
        draw_set_colour(colSelected);
        draw_rectangle(contPauseX,buttonY,contPauseX2,buttonH,false);
        draw_set_color(c_black);
        draw_set_halign(fa_center);
        draw_set_valign(fa_middle);
        draw_text((contPauseX2 - contPauseX) / 2,( buttonY + ((buttonH-buttonY) / 2)),pausedMenu[i,0])
        draw_set_color(c_gray);
        draw_line_width(contPauseX,buttonH-1,contPauseX2,buttonH-1,4);
        }
    else
        {
        draw_set_color(colPrimary);
        draw_rectangle(contPauseX,buttonY,contPauseX2,buttonH,false);
        draw_set_color(c_black);
        draw_set_halign(fa_center);
        draw_set_valign(fa_middle);
        draw_text((contPauseX2 - contPauseX) / 2,( buttonY + ((buttonH-buttonY) / 2)),pausedMenu[i,0])
        draw_set_color(c_gray);
        draw_line_width(contPauseX,buttonH-1,contPauseX2,buttonH-1,4);
        // Checking if mouse is within bounds to make new selection
        if
            (
            mouse_x &gt; contPauseX &amp;&amp;
            mouse_x &lt; contPauseX2 &amp;&amp;
            mouse_y &gt; buttonY &amp;&amp;
            mouse_y &lt; buttonH &amp;&amp;
            selectionActive == 0
            )
            {
            selectionArray[selectionActive,0] = i;
            }
        }
    };
draw_set_color(colBorder)
draw_rectangle( // Drawing Border
    contPauseX,
    contPauseY,
    contPauseX2,
    contPauseY2,
    true
    );

    
    
draw_text(500,500,selectionArray[selectionActive,0])

</string>
